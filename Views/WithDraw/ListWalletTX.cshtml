@using CMSWallet;
@{
    List<listwallethistorywithdraw> ls = Model;
}

<div class="card">
    <div class="card-body">
        <h4 class="card-title">List Wallet</h4>
        <p class="card-description">
            @*Add class <code>.table</code>*@
        </p>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr class="">
                        <th style="color:white">Project</th>
                        <th style="color:white">Address</th>
                        <th style="color:white">TXs</th>
                        <th style="color:white">Option</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        foreach (var item in ls)
                        {
                            <tr>
                                <td style="color:white">@item.projectname</td>
                                <td style="color:white"> @Exten.Tinyhash(item.address)</td>
                                <td style="color:white">@item.countTx</td>
                                <td>
                                    <a class="btn btn-success mdi mdi-history" data-toggle="tooltip" data-placement="top" title="view history" href="@Url.Action("HistoryWalletTX","WithDraw",new {address=item.address , page = 1})"></a>
                                   
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

